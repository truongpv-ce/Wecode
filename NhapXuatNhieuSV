#include <iomanip>
#include <iostream>
#include <stdio.h>
using namespace std;
#define MAXN 100

struct SinhVien {
    char* MASV;
    char* HoTen;
    char NgaySinh[12];
    char GioiTinh;
    float DiemToan, DiemLy, DiemHoa, DTB;
};
void Nhap1SV(SinhVien& a) {
    a.MASV = new char[100];
    a.HoTen = new char[100];
    cin.ignore();
    cin.getline(a.MASV, 100);
    cin.getline(a.HoTen, 100);
    cin >> a.NgaySinh;
    cin >> a.GioiTinh;
    cin >> a.DiemToan >> a.DiemLy >> a.DiemHoa;
    a.DTB = (a.DiemToan + a.DiemLy + a.DiemHoa) / 3;
}
void Xuat1SV(SinhVien a) {
    cout << a.MASV << "\t"
        << a.HoTen << "\t"
        << a.NgaySinh << "\t"
        << a.GioiTinh << "\t"
        << a.DiemToan << "\t"
        << a.DiemLy << "\t"
        << a.DiemHoa << "\t"
        << setprecision(3)
        << a.DTB
        << endl;
}
void Nhap(SinhVien a[], int& n) {
    cin >> n;
    for (int i = 0; i < n; i++) {
        Nhap1SV(a[i]);
    }
}
void Xuat(SinhVien* a, int n) {
    for (int i = 0; i < n; i++) {
        Xuat1SV(a[i]);
    }
}

int main() {
    SinhVien A[MAXN];
    int n;
    Nhap(A, n);
    Xuat(A, n);
    return 0;
}
