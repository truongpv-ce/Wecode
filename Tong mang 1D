// Nhập, xuất, tổng mảng 1D
#include <iostream>
using namespace std;

void Nhap(int*& a, int&n) {
	cin >> n;
	a = new int[n];
	for (int i = 0; i < n; i++) {
		cin >> a[i];
	}
}
int Tong(int* a, int n) {
	int s = 0;
	for (int i = 0; i < n; i++) {
		s+=a[i];
	}
	return s;
}
int main() {
	int* a, n;
	Nhap(a,n);
	cout << Tong(a, n);
	return 0;
}
