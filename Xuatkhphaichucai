###End banned keyword*/

#include <iostream>
#include <bits/stdc++.h>
using namespace std;
#define MAXN 100

int Input(){
    int n;
    cin >> n;
    return n;
}
void NhapMang(char a[], int n){
    for (int i=0; i<n; i++){
        cin >> a[i];

    }
}
void XuatMang(char a[], int n){
    for (int i=0; i<n; i++){
        if (!((a[i]>=65&&a[i]<=90)||(a[i]>=97&&a[i]<=122))){
             cout << a[i] << '\t';
            }
    }
}
//###INSERT CODE HERE -

int main() {
    char a[MAXN];
    int n;
    n = Input(); // Yêu cầu: n được nhập khi gọi hàm Input!
    NhapMang(a, n);
    XuatMang(a, n);
    return 0;
}
